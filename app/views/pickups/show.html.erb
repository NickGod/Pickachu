<p id="notice"><%= notice %></p>

<div class="container">
<h1> <%= @pickup.name %> </h1>

<p>
  <strong>Seller:</strong>
  <%= getUserName(@pickup.user_id) %>
</p>

<p>
  <strong>Name:</strong>
  <%= @pickup.name %>
</p>

<p>
  <strong>Long:</strong>
  <%= @pickup.long.round(2) %>
</p>

<p>
  <strong>Lat:</strong>
  <%= @pickup.lat.round(2) %>
</p>

<p>
  <strong>Type:</strong>
  <%= @pickup.obj_type %>
</p>

<p>
  <strong>Image url:</strong>
  <%= @pickup.image_url %>
</p>

<p>
  <strong>Price:</strong>
  <%= @pickup.price %>
</p>

<p>
  <strong>Start time:</strong>
  <%= @pickup.start_time %>
</p>

<p>
  <strong>End time:</strong>
  <%= @pickup.end_time %>
</p>

<p>
  <strong>Description:</strong>
  <%= @pickup.description %>
</p>
</div>
